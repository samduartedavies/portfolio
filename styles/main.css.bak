#filmsContainer{
	width:100%;
	height:100vh;
}
#filmsTitle{
	text-align:left;
	margin-bottom:20px;
}
#videoPlayer {
	display: table;
	position:relative;
	height:0;
	width:100%;
	overflow:hidden;
	background: #000;
}
#videoPlayer iframe {
	position:absolute;
	top:0;
	left:0;
	width:75%;
	height:100%;
}
#videoPlayer iframe,
	#videoPlayer figcaption {
		display: table-cell;
		vertical-align: top;
}
#videoPlayer figcaption {
	width: 25%;
}
#videoPlayer figcaption a {
	display: block;
	opacity: .5;
	transition: 1s opacity;
}
#videoPlayer figcaption a img,
	figure video {
		width: 100%;
		height: auto;
}
#videoPlayer figcaption a:hover {
	opacity: 1;
}
.videoPlayer{
	width:100%;
	height:100vh;
}
