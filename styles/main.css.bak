@font-face{
	font-family:TheSansExtraLight;
	src:url('thesansextralight.ttf');
}
@font-face{
	font-family:title;
	src:url('curvy.ttf');
}
body {
    width: 100%;
    height: 100%;
  	font-family:TheSansExtraLight,helvetica,arial,freesans,sans-serif;font-size:17px;
    color: rgba(64, 64, 64, 0.9);
    background-color: rgba(197, 222, 215, .6);
	overflow-x:hidden;
}


html {
    width: 100%;
    height: 100%;
}
.aboutArt{
	position:fixed;
	width:50%;
	background-color:red;
	padding-top:200px;
	height:100vh;
	margin-right:0;
}
  #image-scroll {
	left:50%;
	width:50%;
	position:relative;
	margin-left:0;

  }
h1,h2,h3{
    margin: 0 0 35px;
  	font-family:TheSansExtraLight,helvetica,arial,freesans,sans-serif;
    font-weight: 700;
    letter-spacing: 1px;
	color:rgb(74, 177, 118);
	text-align:center;
}
h1{
	font-size:80px;
}
h2 {
	font-size:55px;
	margin-bottom:50px;
	color:#3f7b58;
}
h3{
	font-size:25px;
	color:rgba(73, 177, 155);
}
.img-circle {
    border-radius: 100%;
	width:40%;
    margin: auto;
    top: 0;
    left: 0;
    right: 0;
    margin-bottom: 50px;
}
p {
    margin: 0 0 25px;
    font-size: 19px;
    line-height: 1.5;
    color: rgba(64, 64, 64, 0.9);
	font-family:arial;
    font-weight: 100;	
}
.description{
	line-height: 1;
  	font-family:TheSansExtraLight,helvetica,arial,freesans,sans-serif;font-size:17px;
}
.thin-box{
	max-width:400px;
margin: auto;
    top: 0;
    left: 0;
    right: 0;
	text-align:center;
}
.thick-box{
	max-width:600px;
margin: auto;
    top: 0;
    left: 0;
    right: 0;
	text-align:center;
	padding-bottom:20px;
}
@media(min-width:768px) {
    p {
        margin: 0 0 35px;
        font-size: 19px;
        line-height: 1.6;
    }
}

a {
    color: #42dca3;
    -webkit-transition: all .2s ease-in-out;
    -moz-transition: all .2s ease-in-out;
    transition: all .2s ease-in-out;
}

a:hover,
a:focus {
    text-decoration: none;
    color: #1d9b6c;
}

.light {
    font-weight: 400;
}

.intro {
	margin-bottom:0;;
    display: table;
    width: 100%;
    height: 100vh;
    text-align: center;
	color:rgba(73, 177, 155, 0.85);	
	
}
	#cv-btn{
		float:left;

	}
	#ido-text{
		text-align:left;
	}
.intro-image{
	overflow:hidden;
	width:100%;
	max-width:800px;
	margin: 0 auto;
    left: 0;
    bottom: 0;	
}
.cloud{
	display: inline-block;
	position:absolute;
    -webkit-transition: all 2s ease-out;
    -moz-transition: all 2s ease-out;
    transition: all 2s ease-out;	
}
#clouds{
	left:8%;
	width:270px;
	opacity:.75;
}
#clouds1{
	left:50%;	
	width:180px;	
	opacity:.95;	
}
#clouds2{
	left:75%;
	top:40%;
	width:300px;
	opacity:.8;	
}
@media(max-width:1024px){
	#clouds{
		left:5%;
		width:170px;
	}
	#clouds1{
		left:40%;	
		width:110px;		
	}
	#clouds2{
		top:30%;
		width:200px;		
	}	
}

@media(max-width:800px) {
	#clouds2{
		left:75%;
		top:20%;
		width:180px;	
	}
}
@media(max-width:500px) {
	#clouds{
		left:-15%;
		width:80px;
		top:15%;
	}
	#clouds1{
		left:40%;	
		width:60px;	
		top:10%;		
	}	
	#clouds2{
		left:65%;
		top:25%;
		width:100px;	
	}
}

.intro .intro-body {
    display: table-cell;
    vertical-align: middle;
}

.intro .intro-body .brand-heading {
    font-size: 40px;
}

.intro .intro-body .intro-text {
    font-size: 18px;
}

@media(min-width:768px) {
    .intro {
        height: 100%;
        padding: 0;
    }
	.intro-image{
		width:100%;
		vertical-align: bottom	}

    .intro .intro-body .brand-heading {
        font-size: 100px;
    }

    .intro .intro-body .intro-text {
        font-size: 26px;
    }
}

@-webkit-keyframes pulse {    
    0% {
        -webkit-transform: scale(1);
        transform: scale(1);
    }

    50% {
        -webkit-transform: scale(1.2);
        transform: scale(1.2);
    }

    100% {
        -webkit-transform: scale(1);
        transform: scale(1);
    }
}

@-moz-keyframes pulse {    
    0% {
        -moz-transform: scale(1);
        transform: scale(1);
    }

    50% {
        -moz-transform: scale(1.2);
        transform: scale(1.2);
    }

    100% {
        -moz-transform: scale(1);
        transform: scale(1);
    }
}

.content {
	padding-top:50px;
	
    background-color:#fff;
}

.btn {
    border-radius: 15px;
    text-transform: none;
    font-family: Montserrat,"Helvetica Neue",Helvetica,Arial,sans-serif;
    font-weight: 400;
    -webkit-transition: all .3s ease-in-out;
    -moz-transition: all .3s ease-in-out;
    transition: all .3s ease-in-out;
	margin-top:25px;
}

.btn-websites {
    border-radius: 15px;	
    border: 2px solid #fff;
    color: #fff;
    background-color: transparent;
}

.btn-websites:hover,
.btn-websites:focus {
    border: 2px solid #fff;
    outline: 0;
    color: #000;
    background-color: #fff;
}
.btn-about {
    border-radius: 15px;	
    border: 2px solid #4ab176;
    color: #4ab176;
    background-color: transparent;
}

.btn-about:hover,
.btn-about:focus {
    border: 2px solid #c5ded7;
    outline: 0;
    color: #000;
    background-color: #c5ded7;
}


#hi-section, #ido-section, #help-section {
    width: 100%;
	height:auto;
	margin-top:0;
	color:rgba(0, 179, 60, 0.95);
    background-color: #fff;
}

